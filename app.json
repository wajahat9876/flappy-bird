{
  "expo": {
    "name": "FlappyBird",
    "slug": "FlappyBird",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sprites/birdd.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/sprites/message.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/sprites/birdd.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wajahat959.FlappyBird"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "173217c4-60b6-4129-8b62-6c41268edbf9"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/173217c4-60b6-4129-8b62-6c41268edbf9"
    }
  }
}
